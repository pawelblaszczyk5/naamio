{
	"name": "@naamio/assert",
	"description": "Tiny assert utility",
	"sideEffects": false,
	"type": "module",
	"imports": {
		"#src/*": "./dist/src/*"
	},
	"exports": {
		".": {
			"types": "./dist/src/mod.d.ts",
			"default": "./dist/src/mod.js"
		}
	},
	"scripts": {
		"build": "tsc --noCheck && tsc-alias",
		"dev": "tsc --noCheck && tsc-alias",
		"lint": "eslint",
		"lint:fix": "eslint --fix",
		"typecheck": "tsc --build --noEmit"
	},
	"devDependencies": {
		"@naamio/eslint-config": "workspace:",
		"@naamio/typescript-config": "workspace:",
		"eslint": "catalog:",
		"tsc-alias": "catalog:",
		"typescript": "catalog:"
	}
}
