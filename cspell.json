{
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
	"version": "0.2",
	"import": ["@cspell/dict-pl_pl/cspell-ext.json"],
	"ignorePaths": [
		"pnpm-lock.yaml",
		"cspell.json",
		"**.svg",
		".git/",
		".gitignore",
		".vscode/",
		"LICENSE.md",
		"patches/"
	],
	"dictionaries": ["lorem-ipsum"],
	"useGitignore": true,
	"words": [
		"codegen",
		"codeowner",
		"hmac",
		"millis",
		"naamio",
		"spritesheet",
		"statefulness",
		"typecheck",
		"unredacted"
	],
	"enableGlobDot": true,
	"ignoreWords": [
		"arkregex",
		"fontaine",
		"fontsource",
		"httpapi",
		"lingui",
		"pawelblaszczyk",
		"predev",
		"rolldown",
		"simplewebauthn",
		"stylex",
		"stylexjs",
		"tanstack",
		"taze",
		"timestamptz",
		"tokei",
		"tseslint",
		"vite",
		"vitejs"
	],
	"overrides": [{ "language": "pl-PL,en-US", "filename": "apps/janus/src/locales/pl-PL.po" }]
}
